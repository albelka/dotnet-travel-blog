@model IEnumerable<TravelBlog.Models.Experience>

<h2>Experiences I had</h2>
<ul>
    @foreach (var experience in Model)
    {
        <li>
            @Html.DisplayFor(modelExperience => experience.ExperienceName) | @Html.ActionLink("Details", "Details", new { id = experience.ExperienceId }) | @Html.ActionLink("Edit", "Edit", new { id = experience.ExperienceId })<br />
        @Html.DisplayFor(modelExperience => experience.ExperienceDescription)
    </li>
    }
</ul>

<p>@Html.ActionLink("Add new experience", "Create")</p>